package com.demo.services;

public class ArrayServices {

	public int[] sumOfArray(int[] arr) {
		int[] temp = new int[arr.length];
		for (int i = 0; i < arr.length; i++) {
			int sum = 0;
			for (int j = 0; j < arr.length; j++) {
				if (i != j) {
					sum += arr[j];
				}
			}
			temp[i] = sum;
		}
		return temp;
	}

}
